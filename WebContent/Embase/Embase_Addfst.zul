												<columns>
													<column width="20%" />
													<column />
													<column width="22%" />
													<column />
												</columns>
												<rows>
													<row>
														<cell>
															<hbox>
																<checkbox
																	id="emba_filedebts"
																	onCheck="@command('reqcheck',self=self)" />
																<label
																	value="是否有欠费" />
															</hbox>
														</cell>
														<combobox
															readonly="true" width="80px"
															value="@bind(vm.filedebts)">
															<comboitem />
															<comboitem
																label="是" />
