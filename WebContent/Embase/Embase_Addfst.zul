											<popup id="pop_email">
												点击右键可验证邮箱真实性
											</popup>
											<menupopup
												id="pop_checkemail">
												<menuitem label="验证真实性"
													onClick="@command('checkEmail',email=emba_emailtxt.value)">
												</menuitem>
											</menupopup>
										</hbox>
									</cell>
									<cell>
										<hbox>
											<checkbox id="emba_party"
